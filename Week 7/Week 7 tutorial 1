from pprint import pprint
import requests #import requests to use the library


epqs_url = ('https://epqs.nationalmap.gov/v1/json?'
            'x=%20-75.19128143238117&y=39.95116142515886&units=Feet&wkid=4326&includeDate=False') #the URL of the EPQS site

response = requests.get(url = epqs_url) #the get method allows us to make a GET request to the URL. returns a response object
result = response.json() #returns a python disctionary or list containing response data(only works if response returns a json)

pprint(result, width = 30)

